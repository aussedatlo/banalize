{
  "compilerOptions": {
    "module": "commonjs",
    "importsNotUsedAsValues": "remove",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "ES2021",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,
    "skipLibCheck": true,
    "declaration": true
  }
}

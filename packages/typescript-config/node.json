{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "importsNotUsedAsValues": "remove",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noPropertyAccessFromIndexSignature": false,
    "skipLibCheck": true,
    "declaration": true
  }
}

{
  "name": "@banalize/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "[ -e .env ] && set -a && . ./.env; next dev --turbo --port ${BANALIZE_WEB_PORT-3000}",
    "build": "next build",
    "start": "[ -e .env ] && set -a && . ./.env; next start --port ${BANALIZE_WEB_PORT-3000}",
    "export": "next build && next export",
    "typecheck": "tsc --noEmit",
    "lint": "next lint && pnpm lint:stylelint",
    "lint:fix": "next lint --fix && pnpm lint:stylelint --fix",
    "lint:stylelint": "stylelint '**/*.css'"
  },
  "dependencies": {
    "@mantine/core": "7.12.2",
    "@mantine/form": "^7.12.2",
    "@mantine/hooks": "7.12.2",
    "@tabler/icons-react": "^3.6.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "next": "14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@banalize/eslint-config": "workspace:*",
    "@banalize/stylelint-config": "workspace:*",
    "@banalize/typescript-config": "workspace:*",
    "@types/node": "^20.14.8",
    "@types/react": "18.3.3",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.11",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "stylelint": "^16.6.1",
    "typescript": "5.5.2"
  },
  "packageManager": "yarn@4.4.1"
}
